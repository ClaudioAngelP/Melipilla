		case 3: $tipo_w='(fap_tipo_atencion=3 OR fap_tipo_atencion=4)'; $tfap='ADULTO'; break;
		case 5: $tipo_w='(fap_tipo_atencion=5)'; $tfap='PABELLON'; break;
  	
  	}
  	
